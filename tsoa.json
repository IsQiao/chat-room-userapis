{
    "swagger": {
      "basePath": "/api/v1",
      "entryFile": "./src/app.ts",
      "specVersion": 3,
      "outputDirectory": "./api/dist",
      "controllerPathGlobs": [
        "./src/controllers/**/*controller.ts"
      ]
    },
    "routes": {
      "basePath": "/api/v1",
      "entryFile": "./src/app.ts",
      "routesDir": "./src/routes",
      "middleware": "express"
    }
  }